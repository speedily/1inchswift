{
  "name": "1inchswift",
  "version": "1.0.0",
  "description": "A cross-chain DeFi application built for the ETHGlobal Unite hackathon, extending 1inch Fusion+ to new blockchain networks",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "playwright test",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "1inch",
    "defi",
    "cross-chain",
    "fusion-plus",
    "ethereum",
    "blockchain",
    "hackathon",
    "ethglobal"
  ],
  "author": "1inchSwift Team",
  "license": "MIT",
  "dependencies": {
    "@1inch/cross-chain-sdk": "^0.2.1-rc.58",
    "@1inch/fusion-sdk": "latest",
    "@1inch/limit-order-sdk": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "web3": "^4.0.1",
    "ethers": "^6.7.0",
    "wagmi": "^1.3.0",
    "viem": "^1.19.0",
    "@tanstack/react-query": "^4.29.0",
    "axios": "^1.4.0",
    "react-router-dom": "^6.14.0",
    "react-hook-form": "^7.44.0",
    "zustand": "^4.3.8",
    "clsx": "^1.2.1",
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.24"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jest": "^29.5.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@playwright/test": "^1.35.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "prettier": "^2.8.8"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/1inchswift.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/1inchswift/issues"
  },
  "homepage": "https://github.com/your-username/1inchswift#readme"
} 